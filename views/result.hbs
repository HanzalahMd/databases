<h1 style="text-align: center;">Your Search Results</h1>


<table class="table mt-5">
    <thead class="thead-dark">
        <tr>
            <th scope="col">App ID</th>
            <th scope="col">Name</th>
            <th scope="col">Category</th>
            <th scope="col">Rating</th>
            <th scope="col">Number of Install</th>
        </tr>
    </thead>
    <tbody>
        {{#each display}}
        <tr>
            <td>{{this.app_id}}</td>
            <td>{{this.name}}</td>
            <td>{{this.category}}</td>
            <td>{{this.rating}}</td>
            <td>{{this.installs}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
